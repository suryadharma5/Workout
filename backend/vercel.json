{
    "version": 2,
    "builds": [
        { 
            "src": "*.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/workout/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api/user/(.*)",
            "dest": "server.js"
        }
    ]
}