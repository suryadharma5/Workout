{
    "version": 2,
    "builds": [
        { 
            "src": "*.js", 
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/workout/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/api/user/(.*)",
            "destination": "/server.js"
        }
    ]
}